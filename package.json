{
  "name": "@stellarbeat/js-stellar-node-connector",
  "version": "0.2.0",
  "description": "Connect and send commands to nodes in the stellar network",
  "repository": {
    "type": "git",
    "url": "https://github.com/stellarbeat/js-stellar-node-connector.git"
  },
  "scripts": {
    "flow": "flow",
    "flow:check": "flow check ./src/",
    "flow:deps": "flow-typed install",
    "flow:build": "flow-remove-types ./src/ -d ./lib/ --all --pretty; flow-copy-source src lib",
    "examples:connect": "npm run flow:build; node examples/connect",
    "test": "npm run flow:build; jest"
  },
  "main": "lib/index.js",
  "author": "pieterjan84@github",
  "license": "MIT",
  "dependencies": {
    "@stellarbeat/js-stellar-domain": "github:stellarbeat/js-stellar-domain#master",
    "bignumber.js": "^4.0.0",
    "curve25519-n": "^1.4.0",
    "dotenv": "^6.1.0",
    "ed25519": "0.0.4",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "mysql": "^2.15.0",
    "net": "^1.0.2",
    "npm": "^6.4.1",
    "ramda": "^0.25.0",
    "stellar-base": "0.8.2",
    "tweetnacl": "^1.0.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "flow-copy-source": "^2.0.2",
    "flow": "^0.2.3",
    "flow-bin": "^0.73.0",
    "flow-remove-types": "^1.2.3",
    "flow-typed": "^2.4.0"
  }
}
